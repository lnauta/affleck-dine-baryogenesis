Cell[BoxData[{
 RowBox[{
  RowBox[{"Clear", "[", 
   RowBox[{"A", ",", "b", ",", "n"}], "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"potential", "=", 
   RowBox[{
    SuperscriptBox[
     RowBox[{"Log", "[", 
      RowBox[{"1", "+", 
       FractionBox["\[Chi]", 
        SqrtBox["2"]]}], "]"}], "2"], "-", 
    RowBox[{"(", 
     RowBox[{
      FractionBox[
       RowBox[{
        RowBox[{"Abs", "[", "A", "]"}], 
        SuperscriptBox["b", 
         RowBox[{"2", "-", "n"}]]}], "n"], 
      SuperscriptBox[
       RowBox[{"(", 
        FractionBox["\[Chi]", 
         SqrtBox["2"]], ")"}], "n"]}], ")"}], "+", 
    RowBox[{
     SuperscriptBox["b", 
      RowBox[{"4", "-", 
       RowBox[{"2", "n"}]}]], 
     SuperscriptBox[
      RowBox[{"(", 
       FractionBox["\[Chi]", 
        SqrtBox["2"]], ")"}], 
      RowBox[{
       RowBox[{"2", "n"}], "-", "2"}]]}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
