Cell[CellGroupData[{

Cell["Definitions and Ticks", "Section",
 InitializationGroup->True],

Cell[CellGroupData[{

Cell["Log Ticks", "Subsection",
 InitializationGroup->True],

Cell[BoxData[{
 RowBox[{
  RowBox[{"ClearAll", "[", 
   RowBox[{"LogTicks", ",", "min", ",", "max"}], "]"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"LogTicks", "[", 
   RowBox[{"min_", ",", "max_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "up", ",", "bot", ",", "q", ",", "steps", ",", "major", ",", "minor", 
      ",", "maxlabels", ",", "majorlength", ",", "minorlength", ",", "test"}],
      "}"}], ",", 
    RowBox[{
     RowBox[{"bot", "=", 
      RowBox[{"Floor", "[", 
       RowBox[{"Log10", "@", "min"}], "]"}]}], ";", 
     RowBox[{"up", "=", 
      RowBox[{"Ceiling", "[", 
       RowBox[{"Log10", "@", "max"}], "]"}]}], ";", 
     RowBox[{"q", "=", 
      RowBox[{"Floor", "[", " ", 
       RowBox[{"Log10", "[", 
        RowBox[{"max", "-", "min"}], "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"maxlabels", "=", "7"}], ";", 
     RowBox[{"(*", 
      RowBox[{"max", " ", "labels", " ", "visible", " ", "per", " ", "axis"}],
       "*)"}], "\[IndentingNewLine]", 
     RowBox[{"majorlength", "=", "0.01"}], ";", 
     RowBox[{"minorlength", "=", "0.005"}], ";", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"up", "-", "bot"}], ">", "maxlabels"}], ",", 
       RowBox[{"steps", "=", 
        RowBox[{"Ceiling", "[", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"up", "-", "bot"}], ")"}], "/", "maxlabels"}], "]"}]}], 
       ",", 
       RowBox[{"steps", "=", "1"}]}], "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"major", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{
           RowBox[{"10", "^", 
            RowBox[{"(", "i", ")"}]}], "//", "N"}], ",", 
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{"i", "\[Equal]", "0"}], ",", "1", ",", 
            RowBox[{"DisplayForm", "[", 
             RowBox[{"SuperscriptBox", "[", 
              RowBox[{"10", ",", "i"}], "]"}], "]"}]}], "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"majorlength", ",", "0"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"Black", ",", 
            RowBox[{"AbsoluteThickness", "[", "0.25", "]"}]}], "}"}]}], "}"}],
         ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "bot", ",", "up", ",", "steps"}], "}"}]}], "]"}]}],
      ";", "\[IndentingNewLine]", 
     RowBox[{"Which", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"up", "-", "bot"}], ">", 
        RowBox[{"2", " ", "maxlabels"}]}], ",", "\[IndentingNewLine]", 
       RowBox[{"minor", "=", 
        RowBox[{
         RowBox[{"Table", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{
             RowBox[{"10", "^", 
              RowBox[{"(", "i", ")"}]}], ",", "\"\<\>\"", ",", 
             RowBox[{"{", 
              RowBox[{"minorlength", ",", "0"}], "}"}]}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"i", ",", "bot", ",", "up"}], "}"}]}], "]"}], "//", 
         "N"}]}], ",", "\[IndentingNewLine]", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"up", "-", "bot"}], ">", "maxlabels"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"minor", "=", 
        RowBox[{"Join", "[", 
         RowBox[{
          RowBox[{"Table", "[", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{
              RowBox[{
               RowBox[{"10", "^", 
                RowBox[{"(", "i", ")"}]}], "//", "N"}], ",", "\"\<\>\"", ",", 
              RowBox[{"{", 
               RowBox[{"majorlength", ",", "0"}], "}"}], ",", 
              RowBox[{"{", 
               RowBox[{"Black", ",", 
                RowBox[{"AbsoluteThickness", "[", "0.25", "]"}]}], "}"}]}], 
             "}"}], ",", 
            RowBox[{"{", 
             RowBox[{"i", ",", 
              RowBox[{"bot", "+", "1"}], ",", "up", ",", "2"}], "}"}]}], 
           "]"}], ",", 
          RowBox[{"Flatten", "[", 
           RowBox[{
            RowBox[{"Table", "[", 
             RowBox[{
              RowBox[{"{", 
               RowBox[{
                RowBox[{"d", "*", 
                 RowBox[{"10.", "^", 
                  RowBox[{"(", "i", ")"}]}]}], ",", "\"\<\>\"", ",", 
                RowBox[{"{", 
                 RowBox[{"minorlength", ",", "0"}], "}"}]}], "}"}], ",", 
              RowBox[{"{", 
               RowBox[{"i", ",", "bot", ",", "up"}], "}"}], ",", 
              RowBox[{"{", 
               RowBox[{"d", ",", "2", ",", "9"}], "}"}]}], "]"}], ",", "1"}], 
           "]"}]}], "]"}]}], ",", "\[IndentingNewLine]", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"Log10", "[", "max", "]"}], "-", 
         RowBox[{"Log10", "[", "min", "]"}]}], "\[GreaterEqual]", "1"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"minor", "=", 
        RowBox[{"Flatten", "[", 
         RowBox[{
          RowBox[{"Table", "[", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{
              RowBox[{"d", "*", 
               RowBox[{"10.", "^", 
                RowBox[{"(", "i", ")"}]}]}], ",", "\"\<\>\"", ",", 
              RowBox[{"{", 
               RowBox[{"minorlength", ",", "0"}], "}"}]}], "}"}], ",", 
            RowBox[{"{", 
             RowBox[{"i", ",", "bot", ",", "up"}], "}"}], ",", 
            RowBox[{"{", 
             RowBox[{"d", ",", "2", ",", "9"}], "}"}]}], "]"}], ",", "1"}], 
         "]"}]}], ",", "\[IndentingNewLine]", "\[IndentingNewLine]", "True", 
       ",", "\[IndentingNewLine]", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"(", 
              RowBox[{"max", "-", "min"}], ")"}], "/", 
             SuperscriptBox["10", "q"]}], "<", "2"}], ",", 
           RowBox[{"q", "=", 
            RowBox[{"q", "-", "1"}]}]}], "]"}], ",", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"(", 
              RowBox[{"max", "-", "min"}], ")"}], "/", 
             SuperscriptBox["10", "q"]}], ">", "maxlabels"}], ",", 
           RowBox[{"steps", "=", 
            RowBox[{"Ceiling", "[", 
             FractionBox[
              RowBox[{
               RowBox[{"(", 
                RowBox[{"max", "-", "min"}], ")"}], "/", 
               SuperscriptBox["10", "q"]}], "maxlabels"], "]"}]}], ",", 
           RowBox[{"steps", "=", "1"}]}], "]"}], ",", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"q", "\[GreaterEqual]", "bot"}], ",", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"major", "=", 
              RowBox[{"Table", "[", 
               RowBox[{
                RowBox[{"{", 
                 RowBox[{
                  RowBox[{"d", " ", 
                   SuperscriptBox["10.", "q"]}], ",", 
                  RowBox[{"Which", "[", 
                   RowBox[{
                    RowBox[{"q", "\[Equal]", 
                    RowBox[{"-", "1"}]}], ",", 
                    RowBox[{"d", " ", 
                    SuperscriptBox["10.", "q"]}], ",", 
                    RowBox[{
                    RowBox[{"q", "\[Equal]", "0"}], "||", 
                    RowBox[{"q", "\[Equal]", "1"}]}], ",", 
                    RowBox[{"d", " ", 
                    SuperscriptBox["10", "q"]}], ",", "True", ",", 
                    RowBox[{"If", "[", 
                    RowBox[{
                    RowBox[{"d", "<", "10"}], ",", 
                    RowBox[{"d", "\[Cross]", 
                    RowBox[{"DisplayForm", "[", 
                    RowBox[{"SuperscriptBox", "[", 
                    RowBox[{"10", ",", "q"}], "]"}], "]"}]}], ",", 
                    RowBox[{
                    RowBox[{"(", 
                    RowBox[{"0.1", "d"}], ")"}], "\[Cross]", 
                    RowBox[{"DisplayForm", "[", 
                    RowBox[{"SuperscriptBox", "[", 
                    RowBox[{"10", ",", 
                    RowBox[{"q", "+", "1"}]}], "]"}], "]"}]}]}], "]"}]}], 
                   "]"}], ",", 
                  RowBox[{"{", 
                   RowBox[{"majorlength", ",", "0"}], "}"}], ",", 
                  RowBox[{"{", 
                   RowBox[{"Black", ",", 
                    RowBox[{"AbsoluteThickness", "[", "0.25", "]"}]}], 
                   "}"}]}], "}"}], ",", 
                RowBox[{"{", 
                 RowBox[{"d", ",", "1", ",", 
                  RowBox[{
                   RowBox[{"Ceiling", "[", 
