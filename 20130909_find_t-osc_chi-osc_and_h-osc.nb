    RowBox[{"Tosc", "=", 
     RowBox[{"N", "[", 
      RowBox[{"2", "/", "3"}], "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"Tstart", "=", 
     RowBox[{
      FractionBox["1", "10"], "Tosc"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"Tend", "=", 
     RowBox[{
      RowBox[{"10", "^", "3"}], "  ", "Tstart"}]}], ";", 
